<style type="text/css">
	body {
		margin: 0 auto;
	}
	body, td, th {
		font-family: 'Source Sans Pro', sans-serif;
		font-size: 12px;
	}
	th {
		text-align: center;
	}

	@media screen{
		.table-head{
			width: 70%;
		}
		.table-bordereds{
			border-collapse: collapse;
