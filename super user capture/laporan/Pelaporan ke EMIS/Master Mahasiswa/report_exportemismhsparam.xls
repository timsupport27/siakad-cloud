	@media print{
		@page{
			size: A4 portrait landscape;
		}
		.table-head{
			width: 100%;
		}
		.table-bordereds{
			border-collapse: collapse;
			width: 100%;
		}
		.table-bordereds td,
		.table-bordereds th {
		    border: 1px solid #000!important;
		}

		.table-sign{
			width: 100%;
		}
	}
</style>
<br/>
	<table class="table table-bordereds" align="center" border="1">
